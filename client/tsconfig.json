{
  "extends": "./tsconfig.client",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "global.d.ts",
    "../../shared"
  ]
}
